{
  "name": "microservices",
  "version": "1.0.0",
  "description": "Mongo DB, JWT, Socket.IO, Cron Jobs",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "updateStatus": "node scripts/updateGitStatus.js",
    "prepare": "husky install"
  },
  "author": "Miguel Wayas",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "moment-timezone": "^0.5.33",
    "mongoose": "^5.12.13",
    "node-cron": "^2.0.3",
    "socket.io": "^2.4.1"
  },
  "private": true,
  "husky":{
    "hooks": {
      "applypatch-msg": "echo \"[Husky] applypatch-msg\"",
      "pre-applypatch": "echo \"[Husky] pre-applypatch\"",
      "post-applypatch": "echo \"[Husky] post-applypatch\"",
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  },
  "devDependencies": {
    "husky": "^6.0.0",
    "nodemon": "^2.0.7"
  }
}
